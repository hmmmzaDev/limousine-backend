{
  "name": "expressjs-backend-template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "description": "A generic ExpressJS backend template with user management, authentication, file upload, and notification system",
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "nodemon dist/server.js",
    "build": "tsc",
    "dev": "nodemon ./src/server.ts",
    "watch": "tsc -w",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "expressjs",
    "nodejs",
    "backend",
    "template",
    "mongodb",
    "mongoose",
    "authentication",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "1.10.0",
    "bcrypt": "6.0.0",
    "cloudinary": "2.7.0",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "module-alias": "2.2.3",
    "mongodb": "6.17.0",
    "mongoose": "8.16.0",
    "mongoose-autopopulate": "1.1.0",
    "multer": "2.0.1",
    "multer-storage-cloudinary": "4.0.0",
    "nodemailer": "7.0.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@types/node": "24.0.4",
    "nodemon": "3.1.10",
    "tsconfig-paths": "4.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bcrypt"
    ]
  }
}
