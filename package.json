{
	"name": "limousine-backend",
	"version": "1.0.0",
	"main": "dist/server.js",
	"_moduleAliases": {
		"@": "dist"
	},
	"scripts": {
		"start": "node dist/server.js",
		"start:dev": "nodemon dist/server.js",
		"build": "tsc",
		"dev": "nodemon ./src/server.ts",
		"watch": "tsc -w",
		"vercel-build": "npm run build",
		"test": "vitest",
		"test:run": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"prepare": "husky",
		"type-check": "tsc --noEmit"
	},
	"keywords": [
		"expressjs",
		"nodejs",
		"backend",
		"template",
		"mongodb",
		"mongoose",
		"authentication",
		"typescript"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/stripe": "^8.0.417",
		"axios": "1.10.0",
		"bcrypt": "6.0.0",
		"cloudinary": "2.7.0",
		"cors": "2.8.5",
		"date-fns": "4.1.0",
		"dotenv": "16.5.0",
		"express": "5.1.0",
		"helmet": "8.1.0",
		"jsonwebtoken": "9.0.2",
		"module-alias": "2.2.3",
		"mongodb": "6.17.0",
		"mongoose": "8.16.0",
		"mongoose-autopopulate": "1.1.0",
		"multer": "2.0.1",
		"multer-storage-cloudinary": "4.0.0",
		"nodemailer": "7.0.3",
		"stripe": "^18.4.0",
		"swagger-jsdoc": "6.2.8",
		"swagger-ui-express": "5.0.1",
		"typescript": "5.8.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/multer": "^1.4.12",
		"@types/node": "24.0.4",
		"@types/nodemailer": "^6.4.17",
		"@types/supertest": "^6.0.2",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@vitest/coverage-v8": "^2.1.8",
		"husky": "^9.1.7",
		"nodemon": "3.1.10",
		"supertest": "^7.0.0",
		"tsconfig-paths": "4.2.0",
		"vitest": "^2.1.8"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"bcrypt"
		]
	}
